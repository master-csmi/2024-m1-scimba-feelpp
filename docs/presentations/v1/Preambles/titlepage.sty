%To recognise @ as a letter
\makeatletter

\def\firstauthor#1{\gdef\@firstauthor{#1}}
\def\secondauthor#1{\gdef\@secondauthor{#1}}
\def\firstID#1{\gdef\@firstID{#1}}
\def\secondID#1{\gdef\@secondID{#1}}
\def\firstemail#1{\gdef\@firstemail{#1}}
\def\secondemail#1{\gdef\@secondemail{#1}}
\def\supervisor#1{\gdef\@supervisor{#1}}
\def\submitdate#1{\gdef\@submitdate{#1}}


\def\maketitle{
    \begin{Coupling ScimBa and Feel++}{
    \begin{center}{
    \includegraphics[width=0.3\columnwidth]{images/logo.png}
    \vskip 1in \par
    \LARGE \textbf{\@title}
    \vskip 0.9in \par
\renewcommand{\thefootnote}{\fnsymbol{footnote}}

%Authors
\normalsize{
    \textbf{\@firstauthor}\footnote[1]{: \@firstID,  \texttt{\url{\@firstemail}}}\ \& \
    \textbf{\@secondauthor}\footnote[2]{: \@secondID, :\texttt{\url{\@secondemail}}}\ \ \\ \ \\
    Supervised by \@supervisor
    }


\large{Submitted \@submitdate}
}
\end{center}
}
\end{titlepage}
}

%Refresh footnotes to be numbers
\renewcommand{\thefootnote}{\arabic{footnote}}
\makeatother %Refresh @ 